{
  "name": "cmp",
  "version": "0.0.1",
  "description": "Campaign Management Portal",
  "main": "index.js",
  "entry": [
    "regenerator-runtime/runtime",
    "index.js"
  ],
  "scripts": {
    "start": "NODE_ENV=production node build",
    "trigger": "NODE_ENV=production node build/runIndex.js",
    "blast": "NODE_ENV=production node build/blastIndex.js",
    "picker": "NODE_ENV=production node build/pickerIndex.js",
    "report": "NODE_ENV=production node build/reportIndex.js",

    "dev": "NODE_ENV=development nodemon --exec babel-node src",
    "devWeb": "NODE_ENV=development babel-node src",
    "devBlast": "NODE_ENV=development babel-node src/blastIndex.js",
    "devTrigger": "NODE_ENV=development babel-node src/runIndex.js",
    "devPicker": "NODE_ENV=development babel-node src/pickerIndex.js",
    "devReport": "NODE_ENV=development babel-node src/reportIndex.js",

    "clean": "rm -rf build",
    "build": "npm run clean && babel src -D -d build",
    "debug": "nodemon --inspect --exec babel-node src",

    "initDb": "sequelize-cli init",
    "migrate": "sequelize-cli db:migrate",
    "undomigrate": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all",

    "test": "mocha \"./{,!(node_modules)/**/}*.test.js\" --require babel-polyfill --require esm"
  },
  "author": "Yee Hui Poh <yhpoh91@gmail.com>",
  "license": "ISC",
  "private": true,
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "aws-sdk": "^2.531.0",
    "axios": "^0.18.0",
    "axios-rate-limit": "^1.2.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.18.3",
    "bottleneck": "^2.19.5",
    "compression": "^1.7.4",
    "convert-array-to-csv": "^1.0.10",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "csv-parse": "^4.8.7",
    "csvtojson": "^2.0.10",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-validation": "^1.0.2",
    "helmet": "^3.15.1",
    "http-status": "^1.3.1",
    "ini": "^1.3.5",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "log4js": "^5.1.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "mongoose": "^5.6.4",
    "multer": "^1.4.2",
    "mysql": "^2.17.1",
    "mysql2": "^1.7.0",
    "n-readlines": "^1.0.0",
    "nexmo": "^2.5.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "pusher": "^3.0.0",
    "request": "^2.88.2",
    "sequelize": "^5.19.2",
    "socket.io": "^2.2.0",
    "utf8": "^3.0.0",
    "uuid": "^3.3.3",
    "windows-1252": "^1.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^24.1.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "esm": "^3.2.25",
    "jest": "^24.1.0",
    "mocha": "^6.0.1",
    "nexe": "^3.3.2",
    "nodemon": "^1.18.10",
    "regenerator-runtime": "^0.13.2",
    "sequelize-cli": "^5.5.1"
  }
}
